{
    "install": "npm install",
    "start": "node server.js",
    "watch": {
      "install": {
        "include": [
          "^package\\.json$"
        ]
      },
      "restart": {
        "include": [
          "^server\\.js$",
          "\\.env$"
        ],
        "exclude": [
          "^.data/"
        ]
      },
      "throttle": 1000
    }
  }